{
  "name": "ansidev-blog",
  "version": "2.0.0",
  "author": "Le Minh Tri &lt;ansidev@gmail.com&gt;",
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "gen:feed": "ts-node ./src/cli/genFeed",
    "build": "cross-env NODE_ENV=production vite-ssg build && ts-node ./src/cli/genFeed"
  },
  "dependencies": {
    "@vueuse/core": "9.1.0",
    "@vueuse/head": "0.7.9",
    "github-slugger": "1.4.0",
    "medium-zoom": "1.0.6",
    "nanoid": "4.0.0",
    "nprogress": "0.2.0",
    "prism-theme-vars": "0.2.4",
    "vue": "3.2.37",
    "vue-gtag": "2.0.1",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.26.1",
    "@iconify/vue": "3.2.1",
    "@intlify/vite-plugin-vue-i18n": "6.0.1",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "@types/github-slugger": "1.3.0",
    "@types/markdown-it-link-attributes": "3.0.1",
    "@types/node": "18.7.1",
    "@types/nprogress": "0.2.0",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@vitejs/plugin-vue": "2.3.4",
    "autoprefixer": "10.4.8",
    "cross-env": "7.0.3",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "feed": "4.2.2",
    "gray-matter": "4.0.3",
    "happy-dom": "6.0.4",
    "markdown-it-link-attributes": "4.0.1",
    "markdown-it-shiki": "0.5.1",
    "postcss": "8.4.16",
    "postcss-nested": "5.0.6",
    "prettier": "2.7.1",
    "sass": "1.54.4",
    "shiki": "0.11.1",
    "tailwindcss": "3.1.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "unplugin-vue-components": "0.22.4",
    "vite": "2.9.15",
    "vite-plugin-fonts": "0.6.0",
    "vite-plugin-md": "0.20.2",
    "vite-plugin-pages": "0.25.0",
    "vite-plugin-pwa": "0.12.3",
    "vite-plugin-vue-layouts": "0.7.0",
    "vite-ssg": "0.20.2",
    "vite-ssg-sitemap": "0.3.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "rules": {
      "curly": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "packageManager": "yarn@3.2.2"
}
