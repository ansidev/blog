  "author": "Le Minh Tri &lt;ansidev@gmail.com&gt;",
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "gen:feed": "ts-node ./src/cli/genFeed",
    "build": "cross-env NODE_ENV=production vite-ssg build && ts-node ./src/cli/genFeed"
